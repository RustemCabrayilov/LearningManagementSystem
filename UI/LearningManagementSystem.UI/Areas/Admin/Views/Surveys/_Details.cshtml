@model LearningManagementSystem.Application.Abstractions.Services.Survey.SurveyResponse
<!-- Modal -->
<div class="modal fade" id="detailModal-@Model.Id" tabindex="-1" aria-labelledby="detailModalLabel-@Model.Id" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="deleteModalLabel-@Model.Id">Details</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <p>@Model?.Term?.TermSeason</p>
                <p>@Model?.Term?.EndDate</p>
                <p>@Model?.Term?.StartDate</p>
                @foreach (var item in Model?.Questions)
                {
                    <p>@item?.Description</p>
                    <p>@item?.MaxPoint</p>
                }
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>