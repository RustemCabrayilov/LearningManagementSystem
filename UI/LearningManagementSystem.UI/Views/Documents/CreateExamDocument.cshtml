@model LearningManagementSystem.Application.Abstractions.Services.Document.DocumentByOwner


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
                <form asp-action="CreateExamDocument" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <input hidden name="ownerId" type="text" class="form-control" value="@Model.OwnerId" placeholder="Id">
                        <input hidden name="documentType" type="text" class="form-control" value="@Model.DocumentType" />

                    </div>
                    <div class="form-group">
                        <label  class="control-label">Files</label>
                        <input name="files" type="file" class="form-control" multiple required/>d
                    </div>
                    <button type="submit" class="btn btn-primary" >Submit</button>
                <button class="btn btn-light" asp-controller="Home" asp-action="Index">Cancel</button>
            </form>
        </div>
    </div>
</div>